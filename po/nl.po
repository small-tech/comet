#: data/comet.appdata.xml.in:13
msgid "Comet"
msgstr "Comet"

#: src/Widgets/HeaderBar.vala:20
msgid "First line character limit"
msgstr "Tekenlimiet eerste regel"

#: src/Widgets/HeaderBar.vala:31
msgid "Dogmatic (50)"
msgstr "Dogmatisch (50)"

#: src/Widgets/HeaderBar.vala:34
msgid "GitHub truncation (72)"
msgstr "GitHub-afkapping (72)"

#: src/Widgets/HeaderBar.vala:37
